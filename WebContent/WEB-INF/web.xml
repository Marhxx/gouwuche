<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>demo</display-name>
	<servlet>
		<description>
		</description>
		<display-name>QueryGoodsServlet</display-name>
		<servlet-name>QueryGoodsServlet</servlet-name>
		<servlet-class>com.qhit.servlets.QueryGoodsServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>AddCart</display-name>
		<servlet-name>AddCart</servlet-name>
		<servlet-class>com.qhit.servlets.AddCart</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>RemoveCart</display-name>
		<servlet-name>RemoveCart</servlet-name>
		<servlet-class>com.qhit.servlets.RemoveCart</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>QueryGoodsServlet</servlet-name>
		<url-pattern>/QueryGoodsServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddCart</servlet-name>
		<url-pattern>/AddCart</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RemoveCart</servlet-name>
		<url-pattern>/RemoveCart</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
